

struct s_player
{
	float x,
	float y,
	float vel_x,
	float vel_y,
}

int c_max_tiles = 32;
s8 tiles[c_max_tiles][c_max_tiles];


func int main()
{
	SetTraceLogLevel(LOG_WARNING);
	SetConfigFlags(FLAG_VSYNC_HINT);
	InitWindow(800, 600, "AZEN");

	s_player player;
	player.vel_x = 10;

	while !WindowShouldClose() {
		BeginDrawing();
		ClearBackground(BLACK);

		DrawRectangle(player.x, player.y, 32, 32, RED);

		DrawFPS(16, 16);

		EndDrawing();
	}
}
